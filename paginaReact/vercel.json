{
  "version": 2,
  "builds": [
    { "src": "paginaReact", "use": "@vercel/static-build" },
    { "src": "paginaReact/**", "use": "@vercel/static" },
    { "src": "backend/src/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/formulario", "dest": "/backend/src/index.js" },
    { "src": "/(.*)", "dest": "index.html" }
  ]
}

