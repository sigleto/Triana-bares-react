{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" },
    { "src": "/", "use": "@vercel/static" },
    { "src": "cd../backend/src/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "cd../backend/src/index.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
